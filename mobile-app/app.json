{
  "expo": {
    "name": "TechnoMart",
    "slug": "technomart-mobile-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "web" : {
      "bundler": "metro"
    },
    "icon": "./assets/logo.png",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFE0C2"
    },
    "userInterfaceStyle": "light",
    "updates": {
      "enabled": false,
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "extra": {
      "apiUrl": "https://reqres.in/api",
      "eas": {
        "projectId": "724f128a-e5fa-4d17-b1dc-5358de696900"
      }
    },
    "android": {
      "package": "com.joseph224.technomartmobileapp",
      "softwareKeyboardLayoutMode": "pan",
      "windowSoftInputMode": "adjustResize",
      "permissions": ["CAMERA"],
      "blockedPermissions": []
    },
    "owner": "joseph224"
  }
}
